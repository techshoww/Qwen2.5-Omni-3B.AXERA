{
  "model_type": "ONNX",
  "npu_mode": "NPU3",
  "quant": {
    "input_configs": [
      {
        "tensor_name": "x",
        "calibration_dataset": "calib_token2wav//x.tar",
        "calibration_size": 25,
        "calibration_format":"Numpy"
      },
      {
        "tensor_name": "cond",
        "calibration_dataset": "calib_token2wav//cond.tar",
        "calibration_size": 25,
        "calibration_format":"Numpy"
      },
      {
        "tensor_name": "spk",
        "calibration_dataset": "calib_token2wav//spk.tar",
        "calibration_size": 25,
        "calibration_format":"Numpy"
      },
      {
        "tensor_name": "code",
        "calibration_dataset": "calib_token2wav//code.tar",
